import java.util.Scanner;
public class main{
	

	
	public static void main(String[] args) {
	
	Scanner scan= new Scanner(System.in);
	
	
    System.out.println("Introduce 3 malditos números: ");
    int a = scan.nextInt();
    int b = scan.nextInt();
    int c = scan.nextInt();
	
	System.out.println("El número mas pequeño es: "+min(a,b,c));
	
	}
	
	public static int min(int a, int b, int c)
	{		
		return Math.min(a, Math.min(c, b));
	}
	
}
