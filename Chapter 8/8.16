			int resto = 0;
			
			for(int j = 0; j < a.hugeInteger.length; j++)
			{
				
				for(int i = 0; i < a.hugeInteger.length; i++)
				{
					int f = a.hugeInteger[i] * b.hugeInteger[i] + resto;
				
